<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beifeng.ae.model.CurrencyTypeDimension">
    <resultMap type="com.beifeng.ae.model.CurrencyTypeDimension" id="currencyTypeMapper">
        <id column="id" property="id"/>
        <result column="currency_name" property="currencyName" />
    </resultMap>
    
    <select id="getCurrencyTypeDimension" parameterType="com.beifeng.ae.model.CurrencyTypeDimension" resultMap="currencyTypeMapper">
          SELECT id,currency_name FROM dimension_currency_type
          <where>
             <if test="id != 0">
                 id = #{id}
             </if>
             <if test="currencyName != null and currencyName != ''">
                AND currency_name = #{currencyName}
             </if>
          </where>
     </select>
</mapper>